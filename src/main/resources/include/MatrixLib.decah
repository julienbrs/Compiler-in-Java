    void printMatFloat(float[][] matrice) {
        int i = 0;
        int j = 0;
        print("[");
        while (i < matrice.length) {
            print("[");
            j = 0;
            while (j < matrice[i].length) {
                print(matrice[i][j]);
                if (j != matrice[i].length - 1) {
                    print(", ");
                }
                j = j + 1;
            }
            print("]");
            if (i != matrice.length - 1) {
                print(", ");
            }
            i = i + 1;
        }
        print("]");
    }

