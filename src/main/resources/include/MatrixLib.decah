    // ---------- DEEPCOPY ----------

    float[][] deepCopyFloat(float[][] matrix) {
        float[][] newMatrix;
        float[] line;
        int i = 0;
        int j = 0;
        if (matrix.length == 0) {
            return {};
        }
        newMatrix = new float[matrix.length][]();
        while (i < matrix.length) {
            j = 0;
            line = new float[matrix[i].length]();
            while (j < matrix[i].length) {
                line[j] = matrix[i][j];
                j = j + 1;
            }
            newMatrix[i]=line;
            i = i + 1;
        }
        return newMatrix;
    }

    int[][] deepCopyInt(int[][] matrix) {
