    // ---------- COMPARE ----------

    boolean compareFloat(float[][] A, float[][] B) {
        int i = 0;
        int j = 0;
        if (A.length != B.length) {
            return false;
        }
        while (i < A.length) {
            j = 0;
            if (A[i].length != B[i].length) {
                return false;
            }
            while (j < A[0].length) {
                if (A[i][j] != B[i][j]) {
                    return false;
                }
                j = j + 1;
            }
            i = i + 1;
        }
        return true;
    }

