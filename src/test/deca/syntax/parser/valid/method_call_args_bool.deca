// Parser test, method call with bools parameters

class Test {
    boolean testMethodOr(boolean x, boolean y) {
        return (x || y);
    }
    void testMethodAnd(boolean x, boolean y) {
        return (x || y);
    }
}

{
    boolean a, b;
