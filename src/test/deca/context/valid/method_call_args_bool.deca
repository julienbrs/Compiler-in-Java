// Test context, method call with bool in parameters

class Test {
    boolean testMethodOr(boolean x, boolean y) {
        return (x || y);
    }
    boolean testMethodAnd(boolean x, boolean y) {
        return (x && y);
    }
}

{
    boolean a, b;
    Test test = new Test();
    a = test.testMethodOr(true,false);
    b = test.testMethodAnd(true,false);
}