	LEA 3(GB), R0
	STORE R0, 0(R4)
	PUSH R4
	BSR init.Rational
	POP R4
	STORE R4, 1(R2, R3)
	LOAD 1(LB), R2
	LOAD #1, R3
	ADD R3, R2
	BOV debordement_arithmetique
