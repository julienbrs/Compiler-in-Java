	TSTO #9
	BOV pile_pleine
	ADDSP #6
	PUSH R2
	PUSH R3
	PUSH R4
	PUSH R5
; Variables declarations
	LOAD #0, R2
	STORE R2, 1(LB)
; Beginning of instructions
	LOAD -3(LB), R2
	LOAD #0, R3
	CMP R3, R2
	BGT else.3
	WSTR "Can't create a table of length <= 0"
	WNL
	BRA end.3
else.3:
	LOAD -2(LB), R2
	CMP #null, R2
	BEQ dereferencement_null
	LOAD -3(LB), R3
	STORE R3, 2(R2)
	LOAD -2(LB), R2
	LOAD -3(LB), R3
	ADD #1, R3
	NEW R3, R4
	SUB #1, R3
	STORE R3, 0(R4)
	BRA init.tab.condition.4
init.tab.boucle.4:
	LOAD #null, R5
	STORE R5, 1(R4, R3)
init.tab.condition.4:
	SUB #1, R3
	CMP #-1, R3
	BNE init.tab.boucle.4
	LOAD R4, R3
	STORE R3, 1(R2)
	BRA while_cond.5
