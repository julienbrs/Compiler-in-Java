class Vector {
    protected Rational[] tab;
    protected int length;
    
    void createTab(int length) {
        int i = 0;
        if (length <= 0) {
            println("Can't create a table of length <= 0");
        } else {
            this.length = length;
            tab = new Rational[length]();
            while(i<length){
                tab[i]= new Rational();
                i=i+1;
            }
        }
    }
    Rational[] getTab(){
        return tab;
    }
}

