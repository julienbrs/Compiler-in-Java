; start main program
	TSTO #3
	BOV pile_pleine
	ADDSP #3

; Generation table des methodes
; Table methodes Object
	LOAD #null, R2
	STORE R2, 1(GB)
	LOAD code.Object.equals, R2
	STORE R2, 2(GB)

; Main program
; Variables declarations:
	NEW #4, R2
	BOV tas_plein
	LOAD #3, R0
	STORE R0, 0(R2)
	LOAD #4, R3
	FLOAT R3, R3
	STORE R3, 1(R2)
	LOAD #2, R3
	FLOAT R3, R3
	STORE R3, 2(R2)
	LOAD #1, R3
	FLOAT R3, R3
	STORE R3, 3(R2)
	STORE R2, 3(GB)
; Beginning of main instructions:
	HALT

; Methodes de classe
code.Object.equals:
	TSTO #2
	BOV pile_pleine
	PUSH R2
	PUSH R3
	LOAD -2(LB), R2
	LOAD -3(LB), R3
	CMP R2, R3
	SEQ R0
	POP R3
	POP R2
	RTS
; end main program
; Gestion des erreurs d'executions
pile_pleine:
	WSTR "Débordement de pile"
	WNL
	ERROR
